{
  "name": "@hiyve/basic-example",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "cd server && pnpm run dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "setup": "pnpm install && cd server && pnpm install",
    "packages:dev": "node scripts/toggle-packages.js dev",
    "packages:prod": "node scripts/toggle-packages.js prod",
    "packages:status": "node scripts/toggle-packages.js status"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@hiyve/client-provider": "1.25.0",
    "@hiyve/control-bar": "1.25.0",
    "@hiyve/device-selector": "1.25.0",
    "@hiyve/rtc-client": "1.30.0",
    "@hiyve/utilities": "1.23.0",
    "@hiyve/video-grid": "1.23.0",
    "@hiyve/video-tile": "1.23.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.4.0",
    "vite": "^5.4.0"
  }
